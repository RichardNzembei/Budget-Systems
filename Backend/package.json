{
  "name": "BudgetHair",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "schema:diff": "atlas schema diff --from mysql://budgethair_user:BudgetSystems@localhost:3306/budgethair --to file://schema.hcl --dev-url mysql://budgethair_user:BudgetSystems@localhost:3306/budgethair_dev",
    "schema:apply": "atlas schema apply --env local --auto-approve",
    "schema:apply:dry": "atlas schema apply --env local --dry-run",
    "migrate:new": "atlas migrate diff --env local",
    "migrate:status": "atlas migrate status --env local",
    "migrate:apply": "atlas migrate apply --env local --auto-approve",
    "migrate:apply:prod": "atlas migrate apply --env production"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "socket.io": "^4.8.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
